{
  "timeZone": "Europe/Vienna",
  "dependencies": {
    "enabledAdvancedServices": []
  },
  "exceptionLogging": "STACKDRIVER",
  "runtimeVersion": "V8",
  "oauthScopes": [
    "https://www.googleapis.com/auth/gmail.addons.execute",
    "https://www.googleapis.com/auth/userinfo.email",
    "https://www.googleapis.com/auth/gmail.modify",
    "https://www.googleapis.com/auth/gmail.labels",
    "https://www.googleapis.com/auth/gmail.send",
    "https://www.googleapis.com/auth/gmail.compose",
    "https://www.googleapis.com/auth/script.external_request",
    "https://www.googleapis.com/auth/spreadsheets",
    "https://www.googleapis.com/auth/drive",
    "https://www.googleapis.com/auth/documents"
  ],
  "urlFetchWhitelist": [
    "https://generativelanguage.googleapis.com/"
  ],
  "addOns": {
    "common": {
      "name": "Gmail Support Triage AI",
      "logoUrl": "https://www.gstatic.com/images/icons/material/system/2x/support_agent_black_24dp.png",
      "homepageTrigger": {
        "enabled": true,
        "runFunction": "onHomepage"
      },
      "universalActions": [{
        "label": "Home",
        "runFunction": "showHomepageUniversal"
      }, {
        "label": "API Key",
        "runFunction": "showApiKeyTabUniversal"
      }, {
        "label": "Test Mode",
        "runFunction": "showTestModeUniversal"
      }, {
        "label": "Welcome Flow",
        "runFunction": "showWelcomeFlowUniversal"
      }, {
        "label": "Prompt Editor",
        "runFunction": "showPromptEditorUniversal"
      }, {
        "label": "Live Log",
        "runFunction": "showLiveLogTabUniversal"
      }, {
        "label": "Logs",
        "runFunction": "showLogsTabUniversal"
      }, {
        "label": "Settings",
        "runFunction": "showSettingsTabUniversal"
      }]
    },
    "gmail": {
      "homepageTrigger": {
        "enabled": true,
        "runFunction": "onHomepage"
      },
      "contextualTriggers": [{
        "unconditional": {},
        "onTriggerFunction": "onGmailMessage"
      }]
    }
  }
}